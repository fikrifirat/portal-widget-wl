doctype html
html(lang='en')
  head
    if process.env.NEW_RELIC_LICENSE_KEY
      != newrelic.getBrowserTimingHeader()

    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1")

    title= title

    link(rel="apple-touch-icon" href="")
    link(rel="shortcut icon" type="image/x-icon" href="/images/favicon.ico")

    link(href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css", rel="stylesheet")
    link(href="/click2vox/stylesheets/root.css", rel="stylesheet")
    link(href="//cdnjs.cloudflare.com/ajax/libs/raty/2.7.0/jquery.raty.min.css", rel="stylesheet")
    link(href="//cdn.jsdelivr.net/qtip2/2.2.1/jquery.qtip.min.css", rel="stylesheet")

    script(src="//cdn.rawgit.com/google/code-prettify/master/loader/run_prettify.js" async)
    script(src="//cdn.voxbone.com/voxbone/voxbone-2.1.0.min.js")

    //- load requirejs and its config
    script(src="/click2vox/lib/requirejs/require.js")
    script(src="/click2vox/config.js")

    include tracking-scripts/google
    include tracking-scripts/hotjar

    block extra_head

  body
    .container
      nav.navbar
        .navbar-header
          a.navbar-brand(href='#')
          span.subBrand Voxbone WebRTC Beta
    block content

    footer
      .container
        .col-xs-12.text-center#bottom-footer
          a(target="_blank" href="https://www.voxbone.com/resources/Voxbone_AUP.pdf") Acceptable Use Policy
          a(target="_blank" href="https://www.voxbone.com/privacypolicy.jsf") Privacy Policy
          a(href="/click2vox/known-issues") Known Issues
          a(href="/click2vox/faq") FAQ
          a(href="https://github.com/voxbone-workshop/portal-widget/wiki") Wiki
          a(target="_blank" href="mailto:workshop@voxbone.com?subject=Voxbone Widget Generator Feedback") Send Feedback
          a(target="_blank" href="https://workshop.voxbone.com") A Workshop Project
          a(target="_blank" href="https://www.voxbone.com/" style="border-right: 0px;") Copyright Â©2016, VOXBONE SA
